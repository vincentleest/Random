<script type="text/javascript">

$('.row_data').hover(
	function () {
		$(this).css("background","yellow");
	}, 
	function () {
		$(this).css("background","");
	}
);

$('.curindex').hover(
	function () {
		$(this).css("background","yellow");
	}, 
	function () {
		$(this).css("background","");
	}
);

$(function() {
	$( "#datepicker" ).datepicker(
		{ 
			minDate: "-2M", 
			maxDate: "0", 
			dateFormat: 'ymmdd', 
			showButtonPanel: false, 
			numberOfMonths: 1,
			beforeShowDay: function(day) {
				var day = day.getDay();
				if (day == 0 || day == 6) {
					return [false]
				} else {
					return [true]
				}
			}
		}
	);
});

$("#option_form").validate({
		rules: {
			option_date: {
				required: true,
				minlength: 6,
				maxlength: 6,
				digits: true
			}
		},
		messages: {
			option_date: {
				required: "請選擇日期",
				minlength: "日期格式是 YYMMDD",
				maxlength: "日期格式是 YYMMDD",
				digits: "日期需要輸入數字"
			}
		}
	});
	
$(document).ready(function() {
	$('#option_table').dataTable( {
		"aaSorting": [[ 11, "asc" ]],
		"bFilter": false,
		"iDisplayLength": 20,
		"aLengthMenu": [[20, 50, -1], [20, 50, "All"]],
		"bSort": false,
		"bPaginate": false, 
		"bLengthChange": false,
		"bInfo": false,
		"sPaginationType": "full_numbers",
		"sScrollY": "600px",
		"bScrollCollapse": true,
		"iScrollLoadGap": 20,
		"bAutoWidth": true
	});
	
	$('div.dataTables_scrollBody').scrollTop( $('#option_table tbody tr:eq(51)').position().top );
} );
</script>

